.fui-cell-group {
  background-color: #fff;
  line-height: 1.6;
  overflow: hidden;
  position: relative;
}

.fui-cell-group .fui-cell {
  position: relative;
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  -moz-transition-duration: 300ms;
  transition-duration: 300ms;
  transition-property: background-color;
}

.fui-cell-group .fui-cell:before {
  content: " ";
  position: absolute;
  left: 20rpx;
  right: 20rpx;
  top: 0;
  height: 1px;
  color: #d9d9d9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.fui-cell-group-o:after, .fui-cell-group .fui-cell:first-child:before {
  display: none;
}

.fui-cell-group.fui-cell-click .fui-cell:active,
.fui-cell-group .fui-cell.fui-cell-click:active {
  background: #ececec;
}

.fui-cell-group .fui-cell .fui-cell-text {
  /* -webkit-flex: 1;
  flex: 1; */
  color: #333;
}

.fui-cell-group .fui-cell .fui-cell-remark {
  color: #888;
  text-align: right;
  margin-right: 8rpx;
}

.fui-cell-group .fui-cell .fui-cell-remark:after {
  content: " ";
  display: inline-block;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
  height: 10rpx;
  width: 10rpx;
  border-width: 4rpx 4rpx 0 0;
  border-color: #c8c8cd;
  border-style: solid;
  position: relative;
  top: -1px;
  margin-left: 0.3em;
}

.fui-cell-group .fui-cell .fui-cell-remark.noremark:after {
  display: none;
  margin-left: 0;
}

